<%- include ('../partials/header') %>
<%- include ('../partials/menu') %>
<main>
<div class="container" id="Settings">
    <h1>Settings</h1>
    <div class="row">
        <div class="col-md-4" id="Account-Details">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">Account Details</h5>
                    <p class="card-text">Username:<%= username %></p>
                    <p class="card-text">Email:<%= email %></p>
                </div>
            </div>
        </div>
        <div class="col-md-4" id="PDF-History">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">Uploaded PDF's</h5>
                    <p class="card-text">PDF Title:</p>  /*Call PDF title from database*/
                    <p class="card-text">:Date Uploaded></p> /*Call PDF date uploaded from database*/
                </div>
            </div>
        </div>
    </div>
</div>
</main>

<%- include ('../partials/footer') %>